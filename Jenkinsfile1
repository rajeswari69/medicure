pipeline {
   agent any 
     tools {
       jdk 'JAVA_HOME'
       maven 'M2_HOME'
     }
    stages {
     stage('checkout the project') {
       steps {
          git branch: 'main', url: 'https://github.com/rajeswari69/Medicure-project.git'
       }
     }
     stage('package the application') {
       steps {
          sh 'mvn clean package'
       }
     }
   }
}
